{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["standard"],
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "space-before-function-paren": "off",
    "comma-dangle": ["warn", "always-multiline"],
    "no-unused-vars": "off"
  }
}
